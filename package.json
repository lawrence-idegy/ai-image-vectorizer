{
  "name": "idegy-ai-image-vectorizer",
  "version": "2.0.0",
  "description": "idegy AI-powered image to vector converter for designers",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm install && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "vectorizer",
    "svg",
    "image-to-vector",
    "ai",
    "idegy",
    "designer-tools"
  ],
  "author": "idegy",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "pdfkit": "^0.17.2",
    "potrace": "^2.1.8",
    "replicate": "^0.32.0",
    "sharp": "^0.33.2",
    "svg-to-pdfkit": "^0.1.8",
    "svgo": "^3.3.2",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "ws": "^8.18.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.3",
    "supertest": "^7.0.0",
    "vitest": "^2.1.8"
  }
}
